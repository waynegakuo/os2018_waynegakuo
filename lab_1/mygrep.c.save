#include<stdio.h>
#include<stdlib.h>
#include<string.h>

int main(int argc, char *argv[]) {
	char ch;
	FILE *file;

	file=fopen(argv[2], "r"); //reading from file
	if (file== NULL)
	{
		perror("Error opening file. \n");
		exit (EXIT_FAILURE);
	}

	while((ch=fscanf(file)) !=EOF){
		if (strstr (ch, argv[1])!=NULL){
			printf("%s", ch);
	}
}

}

